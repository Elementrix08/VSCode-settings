{
    // Vim
    "vim.leader": " ",
    "vim.smartRelativeLine": true,
    "editor.cursorSurroundingLines": 8,
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "k"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.handleKeys": {
        "<C-b>": false,
        "<C-s>": false,
        "<C-e>": false
    },
    "vim.normalModeKeyBindings": [
        {
            "before": [
                "g",
                "p",
                "d"
            ],
            "commands": [
                "editor.action.peekDefinition"
            ]
        },
        {
            "before": [
                "g",
                "h"
            ],
            "commands": [
                "editor.action.showDefinitionPreviewHover"
            ]
        },
        {
            "before": [
                "g",
                "i"
            ],
            "commands": [
                "editor.action.goToImplementation"
            ]
        },
        {
            "before": [
                "g",
                "p",
                "i"
            ],
            "commands": [
                "editor.action.peekImplementation"
            ]
        },
        {
            "before": [
                "g",
                "q"
            ],
            "commands": [
                "editor.action.quickFix"
            ]
        },
        {
            "before": [
                "g",
                "r"
            ],
            "commands": [
                "editor.action.referenceSearch.trigger"
            ]
        },
        {
            "before": [
                "g",
                "t"
            ],
            "commands": [
                "editor.action.goToTypeDefinition"
            ]
        },
        {
            "before": [
                "g",
                "p",
                "t"
            ],
            "commands": [
                "editor.action.peekTypeDefinition"
            ]
        },
        {
            "before": [
                "<leader>",
                "o"
            ],
            "commands": [
                "workbench.action.quickOpen"
            ]
        },
        {
            "before": [
                "<leader>",
                "t"
            ],
            "commands": [
                "workbench.action.createTerminalEditor"
            ]
        }
    ],
    "vim.statusBarColors.visual": "#D08770",
    "vim.statusBarColors.visualblock": "#a02236",
    "vim.statusBarColors.visualline": "#9C195B",
    "vim.statusBarColors.replace": "#6f1f45",
    "vim.statusBarColors.insert": "#5E81AC",
    "vim.statusBarColors.normal": "#2E3440",
    "vim.cursorStylePerMode.insert": "block",
    "vim.cursorStylePerMode.normal": "block",
    "vim.cursorStylePerMode.replace": "block",
    "vim.cursorStylePerMode.visual": "block",
    "vim.cursorStylePerMode.visualblock": "block",
    "vim.cursorStylePerMode.visualline": "block",
    "vim.statusBarColorControl": true,
    // Editor
    "editor.suggestSelection": "first",
    "editor.formatOnPaste": true,
    "editor.cursorWidth": 3,
    "editor.smoothScrolling": true,
    "editor.fontFamily": "Fira Code",
    "editor.fontWeight": "500",
    "editor.fontLigatures": true,
    "editor.acceptSuggestionOnEnter": "off",
    "editor.tabCompletion": "on",
    "terminal.integrated.allowChords": false,
    "terminal.integrated.copyOnSelection": true,
    "workbench.list.smoothScrolling": true,
    "workbench.activityBar.iconClickBehavior": "focus",
    "workbench.panel.opensMaximized": "never",
    "window.newWindowDimensions": "fullscreen",
    "window.title": "",
    "task.quickOpen.detail": false,
    "problems.showCurrentInStatus": true,
    "scm.inputFontFamily": "editor",
    "search.showLineNumbers": true,
    "debug.console.fontSize": 17,
    "debug.console.fontFamily": "Fira Code Retina",
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "editor.mouseWheelZoom": true,
    "editor.renderWhitespace": "none",
    "debug.toolBarLocation": "hidden",
    "terminal.integrated.cursorWidth": 5,
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.rightClickBehavior": "default",
    "terminal.integrated.inheritEnv": false,
    "terminal.integrated.allowMnemonics": true,
    "git.ignoreLegacyWarning": true,
    "security.workspace.trust.untrustedFiles": "open",
